{% for study in studies %}
{
  "resourceType": "ImagingStudy",
  "uid": "{{ study.get_any_dicom().read_property("0x20", "0x0D") }}",
  "accession": "{{ study.get_any_dicom().read_property("0x08", "0x50") }}",
  "identifier": "{{ study.get_any_dicom().read_property("0x20", "0x10") }}",
  "modalityList": "{{ study.get_any_dicom().read_property("0x08", "0x61") }}",
  "patient": "{{ study.get_any_dicom().read_property("0x10", "0x10") }}",
  "started": "{{ study.get_any_dicom().read_property("0x08", "0x20") }}",
  "interpreter": "{{ study.get_any_dicom().read_property("0x08", "0x1060") }}",
  "numberOfSeries": "{{ study.get_any_dicom().read_property("0x20", "0x1206") }}",
  "numberOfInstances": "{{ study.get_any_dicom().read_property("0x20", "0x1208") }}",
  "procedureReference": "{{ study.get_any_dicom().read_property("0x08", "0x1032") }}",
  "description": "{{ study.get_any_dicom().read_property("0x08", "0x1030") }}",
  "series": [
    {% for series in study.series %}
    {
      "uid": "{{ series.get_any_dicom().read_property("0x20", "0x0E") }}",
      "number": "{{ series.get_any_dicom().read_property("0x20", "0x11") }}",
      "modality": "{{ series.get_any_dicom().read_property("0x08", "0x60") }}",
      "description": "{{ series.get_any_dicom().read_property("0x08", "0x103E") }}",
      "numberOfInstances": "{{ series.get_any_dicom().read_property("0x20", "0x1209") }}",
      "availability": "{{ series.get_any_dicom().read_property("0x08", "0x56") }}",
      "bodySite": "{{ series.get_any_dicom().read_property("0x18", "0x15") }}",
      "laterality": "{{ series.get_any_dicom().read_property("0x20", "0x60") }}",
      "started": "{{ series.get_any_dicom().read_property("0x08", "0x21") }}",
      "performer": "{{ series.get_any_dicom().read_property("0x08", "0x1050") }}",
      "instance": [
        {% for instance in series.instances %}
        {
          "uid": "{{ instance.read_property("0x08", "0x18") }}",
          "number": "{{ instance.read_property("0x20", "0x13") }}"
          "sopClass": "{{ instance.read_property("0x08", "0x16") }}"
          "title": "{{ instance.read_property("0x70", "0x80") }}"
        }{{ "," if not loop.last }}
        {% endfor %}
      ]
    }{{ "," if not loop.last }}
    {% endfor %}
  ]
}{{ "," if not loop.last }}
{% endfor %}
