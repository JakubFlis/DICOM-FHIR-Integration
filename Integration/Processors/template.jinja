{% for study in studies %}
{
  "resourceType": "ImagingStudy",
  "id": {{ study.get_any_dicom().read_property("ID coords") }}
  "series": [
    {% for series in study.series %}
      { 
        "id": {{ series.path }},
        "modality": {{ series.get_any_dicom().read_property("Modality coords") }}
        "instance": [ 
          {% for instance in series.instances %}
          { 
            "uid": {{ instance.pathToFile }},
            "seriesId": {{ instance.seriesPath }}
          }{{ "," if not loop.last }}
          {% endfor %}
        ]
      }{{ "," if not loop.last }}
    {% endfor %}
  ]
}{{ "," if not loop.last }}
{% endfor %}